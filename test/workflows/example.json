{
  "qualified_name": "example",
  "header": {
    "description": "An example and very simple 'calculation' workflow."
  },
  "input": {
    "ds1": {
      "data_type": "str",
      "description": "1st data source name"
    },
    "ds2": {
      "data_type": "str",
      "description": "2nd data source name"
    },
    "var1": {
      "data_type": "str",
      "description": "1st dataset variable name"
    },
    "var2": {
      "data_type": "str",
      "description": "2nd dataset variable name"
    },
    "start_date": {
      "data_type": "str",
      "description": "first date for the dataset"
    },
    "end_date": {
      "data_type": "str",
      "description": "last date for the dataset"
    },
    "output_file": {
      "data_type": "str",
      "description": "The filename for the result"
    }
  },
  "output": {
    "return": {
      "source": "filter1.return",
      "data_type": "ect.Dataset",
      "description": "A data set with the difference"
    }
  },
  "steps": [
    {
      "id": "open_ds1",
      "op": "ect.ops.io.load_dataset",
      "input": {
        "data_source": "example.ds1",
        "start_date": "example.start_date",
        "end_date": "example.end_date"
      }
    },
    {
      "id": "open_ds2",
      "op": "ect.ops.io.load_dataset",
      "input": {
        "data_source": "example.ds2",
        "start_date": "example.start_date",
        "end_date": "example.end_date"
      }
    },
    {
      "id": "filter1",
      "expression": "ds1.filter([var1])",
      "input": {
        "ds1": "open_ds1",
        "var1": "example.var1",
        "ds2": "open_ds2",
        "var2": "example.var2"
      }
    },
    {
      "id": "save_ds",
      "op": "ect.ops.io.save_dataset",
      "input": {
        "data_set": "filter1",
        "output_file": "example.output_file"
      }
    }
  ]
}
