{
    "qualified_name": "timeseries",
    "header": {
        "description": "This graph wraps the 'timeseries' operation."
    },
    "input": {
        "lat": {
            "parameter": true,
            "data_type": "float",
            "valid_range": [-90, 90],
            "description": "Latitude in degrees"
        },
        "lon": {
            "parameter": true,
            "data_type": "float",
            "valid_range": [-180, 180],
            "description": "Longitude in degrees"
        },
        "method": {
            "parameter": true,
            "data_type": "str",
            "value_set": ["NEAREST", "BILINEAR", "BICUBIC"],
            "description": "Interpolation method"
        }
    },
    "output": {
        "return": {
            "output_of": "timeseries_op.return",
            "data_type": "list",
            "description": "Time series as list of values"
        }
    },
    "nodes": [
        {
            "id": "timeseries_op",
            "op": "test.test_cli.timeseries",
            "input": {
                "lat": { "input_from": "lat" },
                "lon": { "input_from": "lon" },
                "method": { "input_from": "method" }
            }
        }
    ]
}
